<Bindings>
	<Binding name="Toggle Targets +" header="AUTOMAGICALLY" category="ADDONS">
		Automagically_ToggleTargetMode()
	</Binding>
	<Binding name="Toggle Targets -" category="ADDONS">
		Automagically_ToggleTargetModeReverse()
	</Binding>
	<Binding name="Set Targets to 1" category="ADDONS">
		Automagically_SetTargetMode(1)
	</Binding>
	<Binding name="Set Targets to 2" category="ADDONS">
		Automagically_SetTargetMode(2)
	</Binding>
	<Binding name="Set Targets to 3" category="ADDONS">
		Automagically_SetTargetMode(3)
	</Binding>
	<Binding name="Set Targets to (Arcane: 6+, Fire: 5+, Frost: 4)" category="ADDONS">
		Automagically_SetTargetMode(4)
	</Binding>
	<Binding name="Set Targets to (Arcane: 6+, Fire: 7+, Frost: 5+)" category="ADDONS">
		Automagically_SetTargetMode(5)
	</Binding>
</Bindings>
